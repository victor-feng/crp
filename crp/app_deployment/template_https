  upstream  ToonDomain {
    server  IpPort max_fails=0;

	check interval=2000 fall=2 rise=2 timeout=1000 default_down=true type=http;
	check_http_send "HEAD /healthz HTTP/1.0\r\n\r\n";
	keepalive 16;
  }
  server {
      listen 80;
      listen 443;
      ssl on;
      server_name t100ToonDomain;
      access_log  logs/ToonDomain.access.log  main;
      ssl_certificate     /usr/local/nginx/keys_cas/certs/Certificate/Certificate.com.cer;
      ssl_certificate_key   /usr/local/nginx/keys_cas/certs/Certificate/Certificate.com.key;

      location / {
          proxy_pass http://ToonDomain/;
          proxy_set_header Host             $host;
          proxy_set_header X-Real-IP        $remote_addr;
          proxy_set_header X-Forwarded-For  $proxy_add_x_forwarded_for;
          proxy_connect_timeout 60;
          proxy_send_timeout 60;
          proxy_read_timeout 30;
      }  
  }
